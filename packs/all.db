e":false,"rupture":"3","enmain":false,"cou":0,"int":0,"cha":0,"ad":0,"fo":0,"att":0,"prd":"-","pr":0,"prm":0,"mvt":0,"rm":0,"lancerarme":"0","lancerarmedegat":"0","autre":"","name1":"Épreuve","name2":"Dégâts 0-2 m","name3":"Dégâts 3-10 m","name4":"Dégâts 11-15 m","name5":"Fonctionnement","name6":"Explosion","name7":"","epreuve1":"@att-distance+@armefeu","epreuve2":"","epreuve3":"","epreuve4":"","epreuve5":"","epreuve6":"","epreuve7":"","jet1":"d20","jet2":"d6+10","jet3":"d6+8","jet4":"d6+4","jet5":"2d6","jet6":"2d6","jet7":"","epreuvecustom":true,"custom":"","ignorePoidSac":false,"prbouclier":false,"enchantement":false,"relique":false,"limitporteur":"humains, nains, tous les elfes, gobelins, semi-homme (petites armes)","categorie":"Armes","armefeu":true},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"SFOIchF0UTpc8INi":3},"flags":{}}
