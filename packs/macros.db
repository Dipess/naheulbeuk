{"name":"Migration V9 - V10","type":"script","scope":"global","author":"IsvfG2tn1neXfLlE","img":"icons/svg/dice-target.svg","command":"//Maj items\nfor (let item of game.items) {\n  if (item.system.lancerarme != undefined) {\n    await item.update({ \"system.att_arme_jet\": item.system.lancerarme })\n  }\n  if (item.system.lancerarmedegat != undefined) {\n    await item.update({ \"system.degat_arme_jet\": item.system.lancerarmedegat })\n  }\n  let dataItem = {}\n  dataItem.system = {}\n  if (item.type == \"arme\") {\n    if (item.system.formula == \"\" || item.system.formula == \"-\") {\n      if (item.system.prd == \"-\") {\n        if (item.epreuvecustom == false) {\n          dataItem.system.munition = true\n        } else {\n          dataItem.system.armefeu = true\n        }\n        dataItem.system.arme_cac = false\n      } else {\n        if (item.system.prbouclier == false) {\n          dataItem.system.arme_autre = true\n        }\n        dataItem.system.arme_cac = false\n      }\n    } else {\n      if (item.system.att_arme_jet != \"-\") {\n        dataItem.system.arme_distance = true\n        dataItem.system.arme_cac = false\n      }\n    }\n    item.update(dataItem)\n  }\n}\nfor (let actor of game.actors) {\n  for (let item of actor.items) {\n    if (item.system.lancerarme != undefined) {\n      await item.update({ \"system.att_arme_jet\": item.system.lancerarme })\n    }\n    if (item.system.lancerarmedegat != undefined) {\n      await item.update({ \"system.degat_arme_jet\": item.system.lancerarmedegat })\n    }\n    let dataItem = {}\n    dataItem.system = {}\n    if (item.type == \"arme\") {\n      if (item.system.enmain==false){dataItem.system.equipe=false}\n      if (item.system.formula == \"\" || item.system.formula == \"-\") {\n        if (item.system.prd == \"-\") {\n          if (item.system.epreuvecustom == false) {\n            dataItem.system.munition = true\n          } else {\n            dataItem.system.armefeu = true\n          }\n          dataItem.system.arme_cac = false\n        } else {\n          if (item.system.prbouclier == false) {\n            dataItem.system.arme_autre = true\n          }\n          dataItem.system.arme_cac = false\n        }\n      } else {\n        if (item.system.lancerarme != \"-\") {\n          dataItem.system.arme_distance = true\n          dataItem.system.arme_cac = false\n        }\n      }\n      item.update(dataItem)\n    }\n  }\n}\nconsole.log(\"fin traitement des items\")\n\n//Maj PNJ\nfor (let monstre of game.actors) {\n  if (monstre.type == \"npc\") {\n    let mo = duplicate(monstre)\n    mo.system.abilities.ad.bonus_man = mo.system.abilities.ad.bonus\n    mo.system.abilities.ad.bonus = 0\n    mo.system.abilities.att.bonus_man = mo.system.abilities.att.bonus\n    mo.system.abilities.att.bonus = 0\n    mo.system.abilities.cha.bonus_man = mo.system.abilities.cha.bonus\n    mo.system.abilities.cha.bonus = 0\n    mo.system.abilities.cou.bonus_man = mo.system.abilities.cou.bonus\n    mo.system.abilities.cou.bonus = 0\n    mo.system.abilities.fo.bonus_man = mo.system.abilities.fo.bonus\n    mo.system.abilities.fo.bonus = 0\n    mo.system.abilities.int.bonus_man = mo.system.abilities.int.bonus\n    mo.system.abilities.int.bonus = 0\n    mo.system.abilities.prd.bonus_man = mo.system.abilities.prd.bonus\n    mo.system.abilities.prd.bonus = 0\n    mo.system.attributes.esq.bonus_man = mo.system.attributes.esq.bonus\n    mo.system.attributes.esq.bonus = 0\n    mo.system.attributes.mphy.bonus_man = mo.system.attributes.mphy.bonus\n    mo.system.attributes.mphy.bonus = 0\n    mo.system.attributes.mpsy.bonus_man = mo.system.attributes.mpsy.bonus\n    mo.system.attributes.mpsy.bonus = 0\n    mo.system.attributes.pr.bonus_man = mo.system.attributes.pr.bonus\n    mo.system.attributes.pr.bonus = 0\n    mo.system.attributes.prm.bonus_man = mo.system.attributes.prm.bonus\n    mo.system.attributes.prm.bonus = 0\n    mo.system.attributes.rm.bonus_man = mo.system.attributes.rm.bonus\n    mo.system.attributes.rm.bonus = 0\n    monstre.update(mo)\n  }\n}\nconsole.log(\"fin traitement des PNJ\")","ownership":{"default":0,"IsvfG2tn1neXfLlE":3},"flags":{"core":{"sourceId":"Macro.tqamidqrFlFMqm4Q"}},"_stats":{"systemId":"naheulbeuk","systemVersion":"2.0.0.0","coreVersion":"10.290","createdTime":1669382784273,"modifiedTime":1669386954877,"lastModifiedBy":"O2PVKZofn8ACxINz"},"folder":null,"sort":0,"_id":"87yovZjr52NCFHzu"}
